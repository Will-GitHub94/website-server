{
    "extends": "airbnb-base",
    "env": {
        "node": true
    },
    "rules": {
        "quotes": ["error", "double"],
        "indent": ["error", "tab"],
        "linebreak-style": ["error", "unix"],
		"no-tabs": "off",
		"no-console": "off",
		"arrow-body-style": ["error", "always"],
		"no-param-reassign": ["error", {
			"props": false
		}]
    }
}
